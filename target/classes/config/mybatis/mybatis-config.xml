<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE configuration
  PUBLIC "-//mybatis.org//DTD Config 3.0//EN"
  "http://mybatis.org/dtd/mybatis-3-config.dtd">
<configuration>
  
  <settings>
	<setting name="jdbcTypeForNull" value="NULL" />
  </settings>
  
  <!-- typeAlias 타입 별칭 설정 ========================== -->
  <typeAliases>
  	<typeAlias type="com.multi.model.RoomVO" alias="Room"/>
  	<typeAlias type="com.multi.model.GameVO" alias="Game"/>
  	<typeAlias type="com.multi.model.ShopVO" alias="Shop"/>
  	<typeAlias type="com.multi.model.UserVO" alias="User"/>
  	<typeAlias type="com.multi.model.SnsLoginVO" alias="SnsLogin"/>
  	<typeAlias type="com.multi.model.WishListVO" alias="WishList"/>
  </typeAliases>
  
  
  <!-- mapper 정의 ====================================== -->
  <mappers>
    <!-- SQL문을 모아놓는 파일 -->
    <mapper resource="config/mybatis/mapper/RoomMapper.xml"/>
    <mapper resource="config/mybatis/mapper/EvalCafeMapper.xml"/>
    <mapper resource="config/mybatis/mapper/GameMapper.xml"/>
    <mapper resource="config/mybatis/mapper/EvalMemMapper.xml"/>
    <mapper resource="config/mybatis/mapper/BoardSearchMapper.xml"/>
    <mapper resource="config/mybatis/mapper/ClickRoomMapper.xml"/>
    <mapper resource="config/mybatis/mapper/UserMapper.xml"/>
    <mapper resource="config/mybatis/mapper/SocialMapper.xml"/>
    <mapper resource="config/mybatis/mapper/AdminMapper.xml"/>
    <mapper resource="config/mybatis/mapper/ShopMapper.xml"/>
    
  </mappers>
  
</configuration>